    "@types/lodash": "^4.14.165",
    "apisauce": "^2.0.0",
    "lodash": "^4.17.20",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.1.6",
    "mobx-state-tree": "^4.0.2",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-dotenv": "^2.4.3",
    "react-native-geolocation-service": "^5.1.1",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-google-places-autocomplete": "^2.1.2",
