{
  "name": "WeatherApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "build:ios": "detox build -c ios",
    "test:e2e:ios": "detox test -c ios",
    "postinstall": "node ./bin/postInstall",
